<h1>Results Data Export (completed only)</h1>

<% form_tag root_path+'admin/results_list_export' do %>

<table width="100%" border="0" cellspacing="0" cellpadding="0">


<tr><td>Export selected UOS to Email: <input type="radio" name="export" value="Y" checked> Yes <input type="radio" name="export" value="N" > No </td></td>&nbsp;<td></tr>
<tr><td><b>Units of Study Completed:</b></td><td></td></tr>
	<%  i=0
		for @oo in @o  do
			i= i+1	%>
		<tr>
			<td><%= @oo.ucode %>
				<%= check_box('choice_'+i.to_s, 0,{}, @oo.id, '') %>
				<% if @oo.complete==2 %>
					(INC)
				<% end %>	 
			</td>
		<td>&nbsp;</td></tr>
	<% end %>	
</table>
        <input type='hidden' name='i' value=<%= i %>>
		<%= submit_tag "Submit" %>
		<input type=button value="Back" onClick="javascript:window.location='<%= root_path %>admin' ">		
<% end %>		